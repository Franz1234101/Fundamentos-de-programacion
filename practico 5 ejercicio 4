#include <stdio.h>

int main() {
    int h1, m1, s1, h2, m2, s2;
    int t1, t2, diff, h, m, s;

    printf("Ingrese primer horario (HH MM SS): ");
    scanf("%d %d %d", &h1, &m1, &s1);
    printf("Ingrese segundo horario (HH MM SS): ");
    scanf("%d %d %d", &h2, &m2, &s2);

    t1 = h1*3600 + m1*60 + s1;
    t2 = h2*3600 + m2*60 + s2;
    diff = t2 - t1;

    h = diff / 3600;
    diff %= 3600;
    m = diff / 60;
    s = diff % 60;

    printf("Tiempo transcurrido: %d:%02d:%02d\n", h, m, s);
    return 0;
}
