#include <stdio.h>

int main() {
    float a, b, c, d, e, f;
    float det, x, y;

    printf("Ingrese los valores a, b, c, d, e, f (para aX + bY = c y dX + eY = f):\n");
    scanf("%f %f %f %f %f %f", &a, &b, &c, &d, &e, &f);

    det = a*e - b*d;

    if (det == 0)
        printf("El sistema no tiene solucion unica (puede ser indeterminado o incompatible).\n");
    else {
        x = (c*e - b*f) / det;
        y = (a*f - c*d) / det;
        printf("Solucion unica: X = %.2f, Y = %.2f\n", x, y);
    }

    return 0;
}
