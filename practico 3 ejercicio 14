#include <stdio.h>

int main() {
    int n, i;
    long a = 1, b = 1, c;
    printf("Ingrese n: ");
    scanf("%d", &n);

    if(n == 0 || n == 1) {
        printf("F(%d) = 1\n", n);
        return 0;
    }

    for(i = 2; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }

    printf("El numero Fibonacci F(%d) = %ld\n", n, b);
    return 0;
}
